@model IEnumerable<MVCMovie.Models.Register>

@{
    ViewData["Title"] = "AllUsers";
}

<div class="jumbotron jumbotron-fluid" style="background-color:bisque;border-radius:15px">
<h1>AllUsers</h1>

<p>
 
</p>
<table class="table table-striped" >
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Phoneno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Age)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Phoneno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Age)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            
        </tr>
}
    </tbody>
</table>
<p>
    <button type="button" class="btn btn-outline-danger" onclick="location.href = '@Url.Action("Index")'">Go to Home</button>
</p>
    </div>
